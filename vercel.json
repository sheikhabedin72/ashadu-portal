{
  "version": 2,
  "name": "shoheb-space-engine",
  "rewrites": [
    { "source": "/generate", "destination": "/api/ashadu-logic" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Owner", "value": "SHEIKH-MOHAMMED ABEDIN" },
        { "key": "X-Namespace", "value": "SHOHEB.SPACE" }
      ]
    }
  ]
}
